{
  "name": "react-miami-boilerplate",
  "version": "1.0.0",
  "author": "marracheco@gmail.com",
  "description": "A React/Redux boilerplate with Bootstrap.",
  "private": false,
  "scripts": {
    "rest-server": "node server.babel.js",
    "start": "npm run build && webpack-dev-server --history-api-fallback --devtool eval-source-map --hot --inline --progress --colors",
    "build": "npm run clean && webpack --progress --colors",
    "build:prod": "npm run clean && NODE_ENV=production webpack --progress --colors",
    "build-old": "NODE_ENV=production && npm run clean && webpack -p",
    "clean": "rimraf build",
    "lint": "eslint ./",
    "git-update": "git update-index --again"
  },
  "precommit": {
    "run": "git-update",
    "silent": true
  },
  "dependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "css-loader": "^0.21.0",
    "es6-promise": "^3.0.2",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-lodash": "^0.1.4",
    "eslint-plugin-react": "^3.6.3",
    "exports-loader": "^0.6.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.24.0",
    "glob": "^5.0.15",
    "history": "~1.17.0",
    "ignore-styles": "^1.2.0",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.2",
    "jsdom": "^8.2.0",
    "node-fetch": "^1.3.3",
    "node-sass": "^3.4.1",
    "postcss-loader": "^0.7.0",
    "pre-commit": "^1.1.2",
    "prunk": "^1.2.1",
    "query-string": "^4.2.3",
    "raw-loader": "^0.5.1",
    "react": "^0.14.5",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^0.14.7",
    "react-global": "^0.1.8",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.3.0",
    "react-router": "^2.0.0",
    "react-router-bootstrap": "^0.23.0",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.2.1",
    "redux-logger": "^2.5.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^1.0.3",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "webpack-require": "0.0.15"
  },
  "devDependencies": {}
  }
}
